# Medical Device Prediction Dashboard API Configuration
VITE_API_BASE_URL=https://api.meddevice-intelligence.com
VITE_API_VERSION=v1

# API Endpoints
VITE_PRE_MULTICLASS_ENDPOINT=/api/predict/pre-multiclass
VITE_POST_BINARY_ENDPOINT=/api/predict/post-binary
VITE_STATUS_SUMMARY_ENDPOINT=/api/status-summary

# API Authentication (if required)
VITE_API_KEY=your_api_key_here
VITE_API_SECRET=your_api_secret_here

# Application Configuration
VITE_APP_NAME=MedDevice Intelligence
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-Powered Medical Device Safety Analytics

# Feature Flags
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=false

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=false
VITE_CHART_ANIMATION_DURATION=1000

# Cache Configuration
VITE_CACHE_DURATION=300000
VITE_ENABLE_OFFLINE_MODE=false